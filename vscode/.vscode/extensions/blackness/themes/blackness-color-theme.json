        "editorOverviewRuler.addedForeground": "#274029",
        "editorOverviewRuler.deletedForeground": "#7e101e",
        "editorOverviewRuler.modifiedForeground": "#4f6d7a",
        "editorOverviewRuler.errorForeground": "#9d1c2d",

        "gitDecoration.addedResourceForeground": "#274029",
        "gitDecoration.deletedResourceForeground": "#7e101e",
        "gitDecoration.modifiedResourceForeground": "#4f6d7a",
        "gitDecoration.untrackedResourceForeground": "#777777",

        "panel.background": "#020202",
        "panel.border": "#222222",

        "sideBar.background": "#0a0a0a",
        "sideBar.foreground": "#a9a9a9",
        "sideBar.border": "#0a0a0a",
        "sideBarTitle.foreground": "#d0d0d0",
        "sideBarSectionHeader.background": "#222222",
        "sideBarSectionHeader.foreground": "#c4c4c4",

        "statusBar.background": "#0a0a0a",
        "statusBar.foreground": "#a9a9a9",
        "statusBar.border": "#0a0a0a",
        "statusBar.noFolderBackground": "#0a0a0a",
        "statusBar.noFolderForeground": "#a9a9a9",
        "statusBar.debuggingBackground": "#222222",
        "statusBar.debuggingForeground": "#c4c4c4",

        "terminal.ansiBlack": "#020202",
        "terminal.ansiBrightBlack": "#919191",
        "terminal.ansiRed": "#0a0a0a",
        "terminal.ansiBrightRed": "#a9a9a9",
        "terminal.ansiGreen": "#131313",
        "terminal.ansiBrightGreen": "#b7b7b7",
        "terminal.ansiYellow": "#222222",
        "terminal.ansiBrightYellow": "#c4c4c4",
        "terminal.ansiBlue": "#343434",
        "terminal.ansiBrightBlue": "#d0d0d0",
        "terminal.ansiMagenta": "#4c4c4c",
        "terminal.ansiBrightMagenta": "#dedede",
        "terminal.ansiCyan": "#636363",
        "terminal.ansiBrightCyan": "#e9e9e9",
        "terminal.ansiWhite": "#777777",
        "terminal.ansiBrightWhite": "#f6f6f6",
        "terminal.foreground": "#919191",
        "terminalCursor.background": "#020202",
        "terminalCursor.foreground": "#919191",
	},
	"tokenColors": [
        {
            "name": "1",
            "scope": [
                "keyword.control.export", // JS "export".
                "keyword.control.import", // JS "import".
                "keyword.other.package", // Java "package".
                "keyword.other.import", // Java "import".
                "punctuation.definition.annotation", // Java annotation starter "@".
                "punctuation.definition.comment", // Comment indicators like "/***/" and "//".
            ],
            "settings": {
                "foreground": "#222222"
            }
        },
        {
            "name": "2",
            "scope": [
                "comment", // Comment contents.
                "comment.block", // Block comment contents.
                "punctuation.accessor", // Same as "punctuation.section".
                "punctuation.section", // Section punctuations like "{}".
                "storage.modifier", // Modifiers like "private", "final", "implements".
                "storage.modifier.package.java", // Package names.
                "storage.modifier.import.java", // Import definitions.
                "storage.type.annotation", // Java annotations "@~Override".
                "storage.type.class", // JS "class" keyword.
            ],
            "settings": {
                "foreground": "#343434"
            }
        },
        {
            "name": "3",
            "scope": [
                "keyword.control", // Keywords like "return", "break", "for" and Java's "new".
                "keyword.operator", // All types of operators like "==", "||", "!" and "typeof".
                "keyword.operator.new", // JS's "new" keyword.
                "keyword.other.documentation.javadoc", // Javadoc keywords like "@returns".
                "markup.list", // Markup list content.
                "meta.brace", // JS brackets like "if ~()" and "then ~()".
                "punctuation.bracket", // Brackets.
                "punctuation.definition", // Punctuations of definitions like "public MyClass~(final String arg~)".
                "punctuation.separator", // Separators like "super(this~.stuff~, that~.stuff)".
                "punctuation.terminator", // Terminator punctuations like ";".
            ],
            "settings": {
                "foreground": "#4c4c4c",
            }
        },
        {
            "name": "4",
            "scope": [
                "keyword.other.unit", // CSS unit keywords like "px", "%" and "vh".
                "meta.tag", // Normaly all HTML tags but only "=" operators in this setup.
                "storage.type", //  Type definitions like "void", "ArrayList", "let" and "const".
                "storage.type.function", // Function keywords like "function", "=>" and "->".
                "support.type", // Things like "exports" ,"~module.~exports".
            ],
            "settings": {
                "foreground": "#636363",
            }
        },
        {
            "name": "5",
            "scope": [
                "entity.name.section", // Config files' section names.
                "entity.name.tag", // HTML keywords like "div", "br", "p" and "img".
                "entity.other.inherited-class", // Inherited stuff like "extends ~blah" (!JS).
                "variable.language", // Keywords like "this" (couldn't find any other example).
            ],
            "settings": {
                "foreground": "#777777",
            }
        },
        {
            "name": "6",
            "scope": [
                "entity.name.type", // Java's class names and JS' class names and references.
                "entity.other.attribute-name", // HTML's tag properties like "<a ~href="">".
                "keyword.other.definition", // Config files' variable names like "~blah = #000000".
                "meta.object-literal.key",// JS objects' keys like "{~blah: that}".
                "source.embedded.js", // JS doc examples like "@example ~collection.find('username', 'Bob');~".
                "support.type.property-name", // Keys like "meta.object-literal.key" in CSS, JSON and stuff like that.
                "variable.other.jsdoc", // JS doc comment parameter and class field references.
                "variable.parameter", // Parameter names.
            ],
            "settings": {
                "foreground": "#919191",
            }
        },
        {
            "name": "7",
            "scope": [
                "entity.name.function", // Function and method names.
                "markup.underline", // Markup's underlined stuffs.
                "meta.function-call", // Function and method calls like "return ~myArray[0].equals(~s)".
                "meta.method.body", // Almost same as "meta.function-call" except "keyword.control"less.
                "support.class", // JS' class names like "~Object.~defProp()" and "extends ~Map"
                "support.constant", // JS' constants like "Math" but not "console" (class).
                "support.variable", // JS things like "process".
                "support.constant.property-value.css", // CSS property values like "position: ~relative".
                "support.function", // JS methods of "support.variable"s like "Promise#~catch()".
                "support.variable.property", // JS properties of "support.variable"s like "support.function".
                "variable.argument.css", // CSS variables as arguments.
                "variable.css", // CSS variables.
                "variable.object.property", // TS object properties like "readonly ~blah = s".
                "variable.other.constant", // JS Constant variables.
                "variable.other.definition", // Java variable definitions like "final ~blah = s".
                "variable.other.object", // Object references like "this.~blah.doThis()".
                "variable.other.property", // Objects' property references like "this.blah.~that".
                "variable.other.readwrite", // Some JS variables.
            ],
            "settings": {
                "foreground": "#a9a9a9",
            }
        },
        {
            "name": "8",
            "scope": [
                "constant.character.escape", // Escape chars inside strings like "\n".
            ],
            "settings": {
                "foreground": "#b7b7b7",
            }
        },
        {
            "name": "9",
            "scope": [
                "markup.fenced_code.block", // Markup's code blocks.
                "constant", // Constants like "5" and "true".
                "constant.other.color", // Colors.
                "source.shell", // Shell scripts' bodies
                "string", // Strings.
                "string.other.link", // Markdown link strings like "[~blah](https://blah.com)"
            ],
            "settings": {
                "foreground": "#c4c4c4",
            }
        },
        {
            "name": "10",
            "scope": [
            ],
            "settings": {
                "foreground": "#d0d0d0",
            }
        },
        {
            "name": "11",
            "scope": [
                "invalid", // All types of invalid stuff.
                "meta.paragraph", // HTML paragraphs.
                "text", // HTML regular text.
            ],
            "settings": {
                "foreground": "#dedede",
            }
        },
        {
            "name": "12",
            "scope": [
            ],
            "settings": {
                "foreground": "#e9e9e9",
            }
        },
        {
            "name": "13",
            "scope": [
                "markup.bold", // Markup's bold stuffs.
                "markup.inline.raw", // Markup's inline code blocks.
            ],
            "settings": {
                "foreground": "#f6f6f6",
            }
        },
	]
}
